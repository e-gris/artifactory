{
  "name": "exgrizz-artifactory",
  "version": "2.0.12",
  "author": "Bryan Belanger, patched by Eric Griswold",,
  "summary": "Forked from autostructure-artifactory. Installs Artifactory OSS",
  "license": "Apache-2.0",
  "source": "https://github.com/e-gris/artifactory.git",
  "project_page": "https://github.com/e-gris/artifactory",
